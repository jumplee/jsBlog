<% include inc/head %>
	<title>后台首页</title>
  </head>
<body class="skin-blue sidebar-mini">
<div class="wrapper">
    <% include inc/top %>
<% include inc/slideBar %>

    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
            <h1>
               分类
                <small>列表</small>
            </h1>
            <ol class="breadcrumb">
                <li><a href="#"><i class="fa fa-dashboard"></i> 后台</a></li>
                <li class="active">分类</li>
            </ol>
        </section>

        <!-- Main content -->
        <section class="content">
            <% for(var i=0;i<cates.length;i++){%>
                <% var cate=cates[i]%>
                <div>
                   <a href="#"> <%= cate.name%></a>
                    <% if(cate.children){%>
                        <ul>
                            <% for(var j=0;j<cate.children.length;j++){ %>
                                <li>--<%=cate.children[j].name%></li>
                            <%}%>
                        </ul>
                    <%}%>
                </div>

            <%}%>
        </section><!-- /.content -->
    </div><!-- /.content-wrapper -->

    <% include inc/footer %>
</div><!-- ./wrapper -->


  </body>
  <!-- 直接调用全局变量 -->
</html>